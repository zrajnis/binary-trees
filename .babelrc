{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["module-resolver", {
      "root": [
        "./src/",
        "./"
      ],
      "extensions": [ ".js", ".ts", "tsx" ]
    }],
    "react-hot-loader/babel"
  ]
}
